$(document).ready(function(){

  $('#like').click(function(){

    var id = $(this).attr('post_id');

     $.ajax({
       url: "/posts/" + id + '/like',
      dataType: "JSON",
     type: "POST",
     error: function (xhr, ajaxOptions, thrownError) {
           alert(xhr.responseText)
       }

  })


})
})
